{
  "manifest_version": 2,
  "name": "DO Audit Log Scraper",
  "version": "1.0",
  "permissions": ["activeTab", "downloads"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
